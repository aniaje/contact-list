{
    "extends": ["react-app", "react-app/jest"],
    "rules": {
        "no-console": "error",
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": [
            "error",
            { "ignoreRestSiblings": true }
        ],
        "@typescript-eslint/no-explicit-any": "error",
        "prefer-const": "error",
        "no-var": "error",
        "react-hooks/rules-of-hooks": "error",
        "react-hooks/exhaustive-deps": "error"
    }
}
